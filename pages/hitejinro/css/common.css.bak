@charset "utf-8";
/* CSS Document */

/* ++++++++++ Media Query ++++++++++ */
/* -- mobile(start) -- */
@media screen and (max-width: 767px) {

	div.inner_wrap { width: 100%; max-width: 1180px; margin: 0 auto; }
	h2 { font-size: 1.25em; color: #000; text-align: center; }


	header { overflow: hidden; position: fixed; left: 0; top: 0; z-index: 90; width: 100%; height: 80px; transition: height 0.3s; }
	header > div.main_bg { width: 100%; height: 80px; border-bottom: 1px solid #dadada; background: #fff; }
	header > div.main_bg > div.inner_wrap { position: relative; height: 80px; }
	header > div.main_bg > div.inner_wrap > h1 { position: absolute; left: 20px; top: 22px; width: 131px; height: 35px; }
	header > div.main_bg > div.inner_wrap > h1 > a { display: block; width: 100%; height: 100%; background: url("../images/bg_header_logo.png") no-repeat; background-size: cover; font-size: 1px; text-indent: -99999em; }
	header > div.main_bg > div.inner_wrap > div.nav_bg { display: none; position: fixed; left: 0; top: 0; z-index: 98; width: 100vw; height: 100vh; background: rgba(0, 0, 0, 0.7); }
	header > div.main_bg > div.inner_wrap > nav { overflow-y: scroll; display: block; position: fixed; right: 0; top: 0; z-index: 99; width: 78.125vw; height: 100vh; padding: 80px 0 30px 20px; background: #fff; transform: translateX(100%); transition: transform 0.3s; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a { display: block; padding: 10px 0; border-bottom: 1px solid #dadada; background: url("../images/btn_close_M.png") no-repeat 93% center; font-size: 0.875em; color: #143e8d; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a.off { display: block; padding: 10px 0; border-bottom: 1px solid #dadada; font-size: 0.875em; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul { display: block; padding: 10px 0; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul > li > a { display: block; width: 100%; padding: 5px 0 5px 20px; font-size: 0.75em; color: #0096d6; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul { display: none; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a.off + ul { display: block; border-bottom: 1px solid #dadada; }
	header > div.main_bg > div.inner_wrap > button { display: block; position: absolute; top: 32px; right: 15px; z-index: 100; width: 20px; height: 16px; background: url("../images/btn_ui_menu.png") no-repeat center center; background-size: cover; text-indent: -99999em; }
	header > div.sub_bg { display: none; width: 100%; height: 346px; background: rgba(255, 255, 255, 0.8); }

	footer { padding: 50px 20px; background: #6a6a6a; }
	footer > div.inner_wrap > ul { margin: 0 0 50px 0; text-align: center; }
	footer > div.inner_wrap > ul > li { display: inline-block; padding: 0 15px 0 0; line-height: 1; }
	footer > div.inner_wrap > ul > li > a { display: inline-block; font-size: 0.75em; color: #fff; }
	footer > div.inner_wrap > div.info { display: table-cell; width: 320px; vertical-align: middle; text-align: center; font-size: 0.6875em; color: #fff;}
	footer > div.inner_wrap > div.info > p { padding: 0 0 10px 0; }
	footer > div.inner_wrap > div.info > p:last-child { padding: 0; }
	footer > div.inner_wrap > div.f_logo { display: none; width: 230px; vertical-align: middle; }
	footer > div.inner_wrap > div.f_logo > img { width: 100%; }

}
/* -- moblie(end) -- */

/*++++++++++++++++++++++++++*/

/* -- tablet(start) -- */
@media screen and (min-width: 768px) and (max-width: 1024px) {

	div.inner_wrap { width: 100%; max-width: 1024px; margin: 0 auto; }
	h2 { font-size: 1.5em; color: #000; text-align: center; }

	header { overflow: hidden; position: fixed; left: 0; top: 0; z-index: 90; width: 100%; height: 90px; transition: height 0.3s; }
	header > div.main_bg { width: 100%; height: 90px; border-bottom: 1px solid #dadada; background: #fff; }
	header > div.main_bg > div.inner_wrap { position: relative; height: 90px; }
	header > div.main_bg > div.inner_wrap > h1 { position: absolute; left: 20px; top: 19px; width: 187px; height: 50px; }
	header > div.main_bg > div.inner_wrap > h1 > a { display: block; width: 100%; height: 100%; background: url("../images/bg_header_logo.png") no-repeat; background-size: cover; font-size: 1px; text-indent: -99999em; }
	header > div.main_bg > div.inner_wrap > div.nav_bg { display: none; position: fixed; left: 0; top: 0; z-index: 98; width: 100vw; height: 100vh; background: rgba(0, 0, 0, 0.7); }
	header > div.main_bg > div.inner_wrap > nav { overflow-y: scroll; display: block; position: fixed; right: 0; top: 0; z-index: 99; width: 78.125vw; height: 100vh; padding: 90px 0 30px 30px; background: #fff; transform: translateX(100%); transition: transform 0.3s; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a { display: block; padding: 20px 0; border-bottom: 1px solid #dadada; background: url("../images/btn_close.png") no-repeat 93% center; font-size: 1em; color: #143e8d; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a.off { display: block; padding: 20px 0; border-bottom: 1px solid #dadada; font-size: 1em; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul { display: block; padding: 10px 0; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul > li > a { display: block; width: 100%; padding: 10px 0 10px 20px; font-size: 0.875em; color: #0096d6; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul { display: none; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a.off + ul { display: block; border-bottom: 1px solid #dadada; }
	header > div.main_bg > div.inner_wrap > button { display: block; position: absolute; top: 35px; right: 35px; z-index: 100; width: 31px; height: 25px; background: url("../images/btn_ui_menu.png") no-repeat center center; background-size: cover; text-indent: -99999em; }
	header > div.sub_bg { display: none; width: 100%; height: 346px; background: rgba(255, 255, 255, 0.8); }

	footer { padding: 50px 0; background: #6a6a6a; }
	footer > div.inner_wrap > ul { margin: 0 0 50px 0; text-align: center; }
	footer > div.inner_wrap > ul > li { display: inline-block; padding: 0 20px 0 0; line-height: 1; }
	footer > div.inner_wrap > ul > li > a { display: inline-block; font-size: 0.875em; color: #fff; }
	footer > div.inner_wrap > div.info { display: table-cell; width: 950px; vertical-align: middle; text-align: center; font-size: 0.75em; color: #fff;}
	footer > div.inner_wrap > div.info > p { padding: 0 0 10px 0; }
	footer > div.inner_wrap > div.info > p:last-child { padding: 0; }
	footer > div.inner_wrap > div.f_logo { display: none; width: 230px; vertical-align: middle; }
	footer > div.inner_wrap > div.f_logo > img { width: 100%; }

}
/* -- tablet(end) -- */

/*++++++++++++++++++++++++++*/

/* -- desktop(start) -- */
@media screen and (min-width: 1025px) {

	div.inner_wrap { width: 100%; max-width: 1180px; margin: 0 auto; }
	h2 { font-size: 1.875em; text-align: center; }

	@keyframes beer_rt {
		from { transform: rotate(0deg) }

		to { transform: rotate(45deg) }
	
	}

	header { overflow: hidden; position: fixed; left: 0; top: 0; z-index: 90; width: 100%; height: 100px; transition: height 0.3s; }
	header.on { height: 446px; }
	header > div.main_bg { width: 100%; height: 100px; border-bottom: 1px solid #dadada; background: #fff; }
	header > div.main_bg > div.inner_wrap { position: relative; height: 100px; }
	header > div.main_bg > div.inner_wrap > h1 { position: absolute; left: 0; top: 22px; width: 187px; height: 50px; }
	header > div.main_bg > div.inner_wrap > h1 > a { display: block; width: 100%; height: 100%; background: url("../images/bg_header_logo.png") no-repeat; background-size: cover; font-size: 1px; text-indent: -99999em; }
	header > div.main_bg > div.inner_wrap > div.nav_bg { display: none; }
	header > div.main_bg > div.inner_wrap > nav { float: right; }
	header > div.main_bg > div.inner_wrap > nav > ul > li { float: left; text-align: center; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > a { display: inline-block; height: 100px; padding: 0 30px; font-size: 0.875em; color: #143e8d; line-height: 100px; }
	header > div.main_bg > div.inner_wrap > nav > ul > li:hover > a { font-weight: bold; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul { padding: 20px 0; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul > li > a { display: block; width: 100%; padding: 10px 0; font-size: 0.875em; color: #0096d6; }
	header > div.main_bg > div.inner_wrap > nav > ul > li > ul > li > a:hover { font-weight: bold; }
	header > div.main_bg > div.inner_wrap > button { display: none; }
	header > div.sub_bg { width: 100%; height: 346px; background: rgba(255, 255, 255, 0.9); }

	footer { padding: 50px 0; background: #99999; }
	footer > div.inner_wrap > ul { margin: 0 0 50px 0; }
	footer > div.inner_wrap > ul > li { display: inline-block; padding: 0 20px 0 0; line-height: 1; }
	footer > div.inner_wrap > ul > li > a { display: inline-block; font-size: 0.875em; color: #000; }
	footer > div.inner_wrap > div.info { display: table-cell; width: 950px; vertical-align: middle; font-size: 0.75em; color: #000; }
	footer > div.inner_wrap > div.info > p { padding: 0 0 10px 0; }
	footer > div.inner_wrap > div.info > p:last-child { padding: 0; }
	footer > div.inner_wrap > div.f_logo { display: table-cell; width: 230px; vertical-align: middle; }
	footer > div.inner_wrap > div.f_logo > img { width: 100%; }

}
/* -- desktop(end) -- */